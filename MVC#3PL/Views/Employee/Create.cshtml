@model EmployeeViewModel
@{
    ViewData["Title"] = "Create";

}

<h1>Create Employee Page</h1>
<br />
<br />
<form asp-action="Create" enctype="multipart/form-data">
    <partial name="PartialView/EmpEditCreatePartialView" model="Model" />
